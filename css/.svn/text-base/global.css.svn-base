#container{						width:1200px; min-height:100%; font-size:1em; margin:0 auto;}


								
/*Sticky Footer*/
html, body {  					height: 100%; }
#wrap { 						min-height: 100%; height: auto; margin: 0 auto -80px; padding: 0 0 80px;}
#footer {  						height: 80px; }


/*New Sticky Footer*/
html, body {height: 100%;}
#wrap { min-height: 100%; /* equal to footer height */  margin-bottom: -310px; padding:0; }
#wrap:after { content: "";  display: block;}
footer, #wrap:after {  height: 310px; }
		
footer{padding:20px; min-width:1200px; width:100%;  background:#222; font-family:arial; font-size:12px; line-height: 1.5; overflow: hidden;}	
footer, footer *{-webkit-box-sizing: border-box;-moz-box-sizing: border-box;  box-sizing: border-box; color:#aaa;}
footer hr{ 	margin:10px 0; border: 0; height: 1px; background: #555;}
footer a{color: #0065a2; font-weight:bold;}
footer a:hover{ text-decoration:none;}

.foot-wrap{display:block; margin:0 auto; width:1200px}	
	
.foot-header{display:block; margin-bottom:20px; position:relative;}
.footer-logo{width:125px;}
.footer-logo img{width:100%; height:auto; margin-left:-10px;}
.foot-slogan{position: absolute;top: 24px;left: 118px;width: 300px; color:#555;}

.foot-affiliates{display:block;}
.foot-affiliates strong{text-transform:uppercase;}	

.foot-columns{display:block; overflow:hidden; margin-bottom:30px;}
.foot-columns .foot-col1,
.foot-columns .foot-col2,
.foot-columns .foot-col3{float:left; width:400px;}
.foot-columns .foot-col1, 
.foot-columns .foot-col2{padding-right:20px; border-right:1px solid #555 }
.foot-columns .foot-col2,
.foot-columns .foot-col3{padding-left: 20px;}






/*Clear Fix*/
.clearfix:after { 				content: "."; display: block; height: 0; clear: both; visibility: hidden;}
.clearfix {						display: inline-block;}/* Hides from IE-mac \*/
* html .clearfix { 				height: 1%;}
.clearfix {						display: block;}/* End hide from IE-mac */

/* PayPal Form*/
.reserve-form{					width:174px; padding:13px; background: #cdcdcd;  
								border-radius:4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; margin: 0 0 10px 0;}
.reserve-form select{			width:auto; border-color:#AFAFAF!important; }
.reserve-form .group{			width:166px!important; border-color:#AFAFAF!important; font-size:13px;}
.reserve-form .group-size{		width:20px!important; border-color:#AFAFAF!important; text-align:center;}
.reserve-form .buy-package{		margin-bottom:5px!important;}
.reserve-form select, 
.reserve-form option{			font-size:13px; line-height:17px;}
