/*2 cols*/

.content .left{					width:292px; float:left;}
.content .right{				width:292px; float:right;}

/* 
	*basic form styles 
*/

.content .form{					width:65%; float:left; padding-right:20px;
								-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;}
								
