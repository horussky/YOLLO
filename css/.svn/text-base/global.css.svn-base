/* CSS Document */
@import url(http://fonts.googleapis.com/css?family=PT+Serif:700);
@import url(http://fonts.googleapis.com/css?family=PT+Serif+Caption);
@import url(http://fonts.googleapis.com/css?family=Sansita+One);
/*
	 RESET
*/
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td {
								margin: 0; padding: 0; border: 0; outline: 0; font-size: 100%; 
								vertical-align: baseline; background: transparent;}
html{							font-size:16px;}
body{							line-height:1;}
ol, ul{							list-style:none;}
table{							border-collapse:collapse; border-spacing:0;}
ins{							text-decoration:none;}
del{							text-decoration:line-through;}
input[type=hidden]{				display:none; border:none;}
blockquote, q{					quotes:none;}
blockquote:before, 
blockquote:after, 
q:before, 
q:after{						content:''; content:none;}
input[type=checkbox],
input[type=radio]{				border:none;}
img[align=left]{				margin:0 10px 6px 0; float:left;}
img[align=right]{				margin:0 0 6px 10px; float:right;}


/*
	 *Structure
*/
html { 							font-size: 100%; overflow-y: scroll; -webkit-text-size-adjust: 100%; -ms-text-size-adjust: 100%; }

a{								color:#0065a2; text-decoration:underline;}
a:hover{						text-decoration:underline; color:#9E1421;}
h2{								text-transform:uppercase; font:30px/25px Arial, Helvetica, sans-serif; margin-bottom:5px; color:#222;}
h3{								font:bold 18px/20px Arial, Helvetica, sans-serif; margin-bottom:5px; color:#222;}
h4{								font:bold 16px/20px Arial, Helvetica, sans-serif; margin-bottom:5px; color:#222;}
body{							background: #fff;}

p{								font:14px Helvetica, Arial,Tahoma, Geneva, sans-serif; line-height:1.5; margin-bottom: 12px;}

/*#container{						width:810px; font-size:1em; float:left;}*/


/* LEFT CONTENT */

#left{							width:20%; float:left;}
#left .nav{						width:100%;}

#left .nav ul{ 					margin:0; padding:0; display:block;}

#left .nav li{					width:5px; height:30px; color:#191919; text-align:center; overflow:hidden; display:block; 
								background:#9E1421;}

#left .nav a{ 					color:#000; font:11px/30px "Arial Black", Gadget, sans-serif; text-decoration:none; text-transform:uppercase; 
								display:block; width:250px; height:30px;  text-align:left; text-indent: 15px; position:absolute;}
#left .nav a:hover{				text-decoration:none; color:#fff; font-weight:bold;}
#left .nav .active, 
#left .nav .active a{ 			color:#9E1421!important; cursor: default; font-weight:bold; text-decoration:none}
#left .nav .active{				background:#000; width:5px;}
									
									
/* RIGHT CONTENT */									

#right{							width:60%; padding:20px; padding-top:0; float:left; 
								-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;
}

	#right hr{					border: none 0; border-top:2px solid #BF1E2E; width: 600px; height: 0; margin:10px 0; clear:both; }
	#right .logo{				width:100%; text-align:center}
	#right .logo img{			margin:0 auto; width:50%;}
	/*#right .logo img{			width:300px;}*/ 


	#toplogo{					width:600px; margin:0 auto;}
	#toplogo img.logo{ 			text-align:center;}
	.content{ 					width:100%; float:left;  font-family: Helvetica, Arial,Tahoma, Geneva, sans-serif; font-size: 14px; margin-bottom:20px;} 
	
	
	
	.content ul{				display:block; font-family: Helvetica, Arial,Tahoma, Geneva, sans-serif; font-size:14px; line-height:1.5; margin-bottom:20px }
	.content li{				list-style:outside disc; margin-left:10px; margin-top:3px; }	

	p.subheader{				font:1.18em/1.5em Georgia, "Times New Roman", Times, serif;}
	
	/*Right Main | Left Column*/
	#right .left-col{			width:50%; float:left; padding-right:20px; 
								-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box;						}
	
	#right .topcontent{			width:100%; float:left; padding:0; clear:both; margin-bottom:10px;}
	
	#right .topcontent img{		width:100%; -moz-border-radius: 10px 10px 0 0; 
								-webkit-border-radius: 10px 10px 0 0; border-radius: 10px 10px 0 0;}
	
	
	.topinfo, 
	.left-col .about{			width:265px; float:left;}
	

	#right .credit{				width:600px; text-align:center;  margin:15px 0 0 0; clear:both;}

	/*Right Main | Right Column*/
	#right .right-col{			width:50%; float:right; padding:0;}
	
	
	#slide { 					width:100%;  background: #eee; float:right; padding:8px; 
								box-shadow: 0 0 4px #aaa; -moz-box-shadow: 0 0 4px #aaa; -webkit-box-shadow: 0 0 4px #aaa; 
								-webkit-box-sizing: border-box; -moz-box-sizing: border-box; box-sizing: border-box; clear:both;}
								
	#slider{					background:#000;}
	
	/* Newsletter */

	#newsletter{				margin:20px 0; width: 100%; position:relative; float:left;}
	
	
/* ASIDE */

	aside#social{				width: 20%; float:left;}
	#social .tweet{				width: 100%; background:none; padding:0; border:none;}	
	
	
	
	/*supersized*/
	#supersized { 				position:fixed; top:0; overflow:hidden; z-index:999999999; }
	#supersized{ 				position:relative;  width:300px; height:208px;}
	#supersized > li{			position:relative; display:none;}
	#supersized img{ 			position:relative; display:none; outline:none; border:none; }
	#supersized a { 			z-index:-30; position:absolute; overflow:hidden; top:0; left:0; width:100%; height:100%; }
	#supersized a.activeslide { z-index:-10;  }
	#supersized a.activeslide img{ display:inline; }
	#supersized.speed img { 	-ms-interpolation-mode:nearest-neighbor; image-rendering: -moz-crisp-edges; }	/*Speed*/		
 	
			
			



