/*Media Queries*/
@media only screen and (max-width: 970px) {
#footer .footer-right, #footer .footer-nav{margin:0;}
}

@media only screen and (max-width: 820px) {
	.tweet, .facebook{									display:none;}
	#footer{											width:100%;}
	#footer .fcontent {									width:100%; }
	#footer .fcontent .fcol1{							width:35%; }
	#footer .fcontent .fcol2{							width:55%;}

article, aside, figcaption, figure, footer, header, hgroup, nav, section
{display: block;}

}
